[
  {
    "name": "Initial",
    "rule": {
      "FIND": {
        "name": "Bill"
      }
    }
  },
  {
    "name": "First",
    "rule": {
      "FIND": {
        "r'^auth[a-zA-Z]+$'": [
          "administrator"
        ]
      }
    }
  },
  {
    "name": "Second",
    "rule": {
      "OR": [
        {
          "FIND$": {
            "office": "r'^[0-9]+$'"
          }
        },
        {
          "AND": [
            {
              "MATCH": {
                "authLevel": ["administrator"],
                "department": ["Technical"]
              }
            }
          ]
        }
      ]
    }
  },
  {
    "name": "Third",
    "rule": {
      "AND": [
        {
          "MATCH": {
            "office": "20"
          }
        },
        {
          "AND": [
            {
              "MATCH": {
                "bindings": {
                  "authLevel": ["administrator"]
                }
              },
              "FIND$": {
                "department": ["Technical"]
              }
            }
          ]
        },
        {
          "AND": [
            {
              "MATCH": {
                "bindings": {
                  "authLevel": ["basic"]
                }
              }
            },
            {
              "OR": [
                {
                  "FIND": {
                    "authLevel": ["administrator"]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  },
  {
    "name": "Fourth",
    "rule": {
      "AND": [
        {
          "MATCH": {
            "office": "r'^[0-9]+$'"
          }
        },
        {
          "FIND": {
            "r'^auth[a-zA-Z]+$'": ["r'^admin[a-z0-9]+$'"],
            "area": [
              "agents"
            ]
          }
        },
        {
          "OR": [
            {
              "MATCH$": {
                "name": "Bill",
                "office": "20"
              }
            },
            {
              "FIND": {
                "department": ["Commercial"],
                "OR": [
                  {
                    "authLevel": ["administrator"],
                    "department": ["Technical"]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  },
  {
    "name": "Fifth",
    "rule": {
      "OR": [
        {
          "AND": [
            {
              "MATCH": {
                "office": "r'^[0-9]*'$"
              }
            }
          ]
        },
        {
          "AND": [
            {
              "authLevel": ["administrator1"]
            },
            {
              "department": ["Technical1"],
              "authLevel": ["basic1"]
            }
          ]
        },
        {
          "AND": [
            {
              "authLevel": ["basic1"],
              "office": "211"
            },
            {
              "department": ["Commercial1"],
              "OR": [
                {
                  "authLevel": ["administrator1"],
                  "department": ["Technical1"]
                }
              ]
            }
          ]
        }
      ]
    }
  }
]